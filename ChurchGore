cyoAdventure = function (a) {
ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
/// Chappaquidick wins nomination, so sad
if(ans == 3547){
    campaignTrail_temp.candidate_json[1]["fields"].first_name = "Ted";
    campaignTrail_temp.candidate_json[1]["fields"].last_name = "Kennedy";
    campaignTrail_temp.question_number = 24;
}
/// Senator Boeing wins nomination, time to bomb Vietnam
if(ans == 3548){
    campaignTrail_temp.candidate_json[1]["fields"].first_name = "Scoop";
    campaignTrail_temp.candidate_json[1]["fields"].last_name = "Jackson";
    campaignTrail_temp.question_number = 24;
}
}

